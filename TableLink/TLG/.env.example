# Naver Map API Key
VITE_NAVER_MAP_API_KEY=your_api_key_here

# Backend API URL (로컬 개발: http://localhost:5000/api/auth)
VITE_API_URL=http://localhost:5000/api/auth

# PostgreSQL Database (Backend에서 사용)
DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require&channel_binding=require

# JWT Secret Keys (반드시 안전한 랜덤 문자열로 변경하세요)
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-this-in-production

# JWT Token Expiry
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d
